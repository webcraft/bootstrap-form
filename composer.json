{
    "name": "nocodelab/bootstrap-form",
    "description": "Laravel 5 form wrappers for Bootstrap 3.",
    "keywords": [
        "laravel",
        "form",
        "bootstrap"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dwight Watson",
            "email": "dwightwatson@me.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/config": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/session": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "rdx/laravelcollective-html": "^6.4|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.1",
        "phpunit/phpunit": "^9.5|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Watson\\BootstrapForm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Watson\\BootstrapForm\\BootstrapFormServiceProvider"
            ],
            "aliases": {
                "BootForm": "Watson\\BootstrapForm\\Facades\\BootstrapForm"
            }
        }
    }
}
